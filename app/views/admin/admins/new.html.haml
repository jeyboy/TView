.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", admin_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_admin_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Group
    .inner
      = form_for :admin, :url => admin_admins_path, :html =>{ :multipart => true } do |f|
        = render :partial => "form_new", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
